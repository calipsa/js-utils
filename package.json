{
  "name": "@calipsa/utils",
  "version": "1.3.0",
  "description": "JavaScript utils",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "lint": "eslint './src/**/*.ts'",
    "build:commonjs": "tsc --outDir dist",
