    "build": "rm -rf dist && run-p -c \"build:*\"",
    "release": "np --yolo --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calipsa/js-utils"
  },
  "keywords": [
    "js",
    "utils"
  ],
  "author": "Anton Veselev",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4"
  },
  "devDependencies": {
    "@calipsa/eslint-config-typescript": "^1.12.0",
    "@types/jest": "^26.0.16",
    "@types/lodash": "^4.14.165",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "np": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
